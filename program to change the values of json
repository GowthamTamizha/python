      "bunitIdKey": "bunit",
      "startDateKey": "startDate",
      "endDateKey": "endDate"
    }
  }
]

    print(f"Total reports available: {len(data)}")

    def modify_report(report, key, new_value):
        """Applies the new value to the specified key in a single report."""
        if key not in report:
            print(f" Key '{key}' not found in report '{report['reportName']}'. Skipping.")
            return False

        current_value = report[key]
        print(f"\n Updating '{key}' in '{report['reportName']}'...")
        print(f"  - Current value: {current_value}")
        print(f"  - Current type: {type(current_value).__name__}")

        try:
            if isinstance(current_value, bool):
                report[key] = new_value.lower() in ['true', 'yes', '1', 't']
            elif isinstance(current_value, (int, float)):
                if isinstance(current_value, int):
                    report[key] = int(new_value)
                else:
                    report[key] = float(new_value)
            elif isinstance(current_value, list):
                report[key] = [item.strip() for item in new_value.split(',')]
            elif isinstance(current_value, dict):
                report[key] = json.loads(new_value)
            else:
                report[key] = new_value

            print(f"  -  Successfully updated to: {report[key]}")
            return True
        except (ValueError, json.JSONDecodeError) as e:
            print(f"  -  Error applying value: {e}. Value not changed.")
            return False

    # Main logic
    print("\nAvailable Reports:")
    for i, report in enumerate(data, 1):
        print(f"{i}. {report['reportName']} - {report['displayName']}")

    try:
        selection_input = input(
            "\nEnter report number(s) to modify (e.g., '1' for single, '1,3' for multiple, or 'all'): ").strip()

        if selection_input.lower() == 'all':
            reports_to_modify = data
            print(f" Selected **all** {len(data)} reports.")
        elif ',' in selection_input:
            indices = [int(i.strip()) - 1 for i in selection_input.split(',')]
            if any(i < 0 or i >= len(data) for i in indices):
                print(" Invalid report number(s)!")
                return
            reports_to_modify = [data[i] for i in indices]
            print(f" Selected **{len(reports_to_modify)}** reports.")
        else:
            report_index = int(selection_input) - 1
            if report_index < 0 or report_index >= len(data):
                print(" Invalid report number!")
                return
            reports_to_modify = [data[report_index]]
            print(f" Selected a **single** report.")

    except (ValueError, IndexError):
        print(" Please enter valid number(s) or 'all'!")
        return

    # Get the key and new value
    print(f"\nAvailable keys for the first selected report: {list(reports_to_modify[0].keys())}")
    key_to_change = input("\nEnter the key you want to change: ")
    new_value_input = input(f"Enter the new value for '{key_to_change}': ")

    # Apply the modification to all selected reports
    print("\n--- Applying changes ---")
    modified_count = 0
    for report in reports_to_modify:
        if modify_report(report, key_to_change, new_value_input):
            modified_count += 1

    print("\n--- Final Status ---")
    print(f"Updated {modified_count} out of {len(reports_to_modify)} selected reports.")
    print("Updated data:")
    print(json.dumps(data, indent=2))


change_key_value()
