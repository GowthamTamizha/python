// Code


def trun_right():
    turn_left()
    turn_left()
    turn_left()
    
def incrementby2():
    move()
    turn_left()
    move()
    trun_right()
    move()
    trun_right()
    move()
    turn_left()

for i in range (6):
    incrementby2()
